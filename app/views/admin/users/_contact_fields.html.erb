<div class="nested-fields">
  <div class="col-lg-12">
    <%= f.label :cell_number %>
    <%= f.text_field :cell_number, class: 'form-control input-cell-number' %>
  </div>
  <div class="col-lg-12">
    <%= f.label :telephone_number %>
    <%= f.text_field :telephone_number, class: 'form-control input-telephone-number' %>
  </div>
  <div class="col-lg-12">
    <%= f.label :email %>
    <%= f.email_field :email, class: 'form-control' %>
  </div>
  <div class="col-lg-12">
    <%= f.label :observation %>
    <%= f.text_field :observation, class: 'form-control' %>
  </div>
  <div class="mt-2">
    <%= link_to_remove_association('Excluir', f, class: 'btn btn-sm -transparent-danger col-lg-12 mb-2') %>
  </div>
</div>
